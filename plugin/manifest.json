{
  "manifest_version": 2,
  "name": "Service Desk Pleasant Lawyer",
  "description": "Quickly go to a Service Desk record by number or two-word phrase from the omnibox",
  "version": "0.6.1",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "omnibox": {"keyword": "b"},
  "content_scripts": [
    {
      "matches": ["https://desk.gotoassist.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
